{
    "root": "../modules/",

    "modules": {
        "main": "main.js",


        "testcase_lmd_basic_features": "test_case/testcase_lmd_basic_features/testcase_lmd_basic_features.js",
        "testcase_lmd_async_require": "test_case/testcase_lmd_async_require/testcase_lmd_async_require.js",
        "testcase_lmd_loader": "test_case/testcase_lmd_loader/testcase_lmd_loader.js",
        "testcase_lmd_coverage": "test_case/testcase_lmd_coverage/testcase_lmd_coverage.js",
        "testcase_lmd_cache": "test_case/testcase_lmd_cache/testcase_lmd_cache.js",
        "testcase_lmd_amd":"test_case/testcase_lmd_amd/testcase_lmd_amd.js"

    },

    "version": "latest",
    "main": "main",
    "global": "this",

    "depends": "*.lmd.json",

    "lazy": false,
    "pack": true,
    "pack_options": {
        "is_mangle": true,
        "compress_options": {
            "sequences"     : true,
            "properties"    : true,
            "dead_code"     : true,
            "drop_debugger" : true,
            "unsafe"        : true,
            "conditionals"  : true,
            "comparisons"   : true,
            "evaluate"      : true,
            "booleans"      : true,
            "loops"         : true,
            "unused"        : false,
            "hoist_funs"    : true,
            "hoist_vars"    : false,
            "if_return"     : true,
            "join_vars"     : true,
            "cascade"       : true,
            "side_effects"  : false,
    
            "warnings"      : true,
            "global_defs"   : {}
        },
        "gen_options": {
            "indent_start"  : 0,
            "indent_level"  : 4,
            "quote_keys"    : false,
            "space_colon"   : true,
            "ascii_only"    : false,
            "inline_script" : false,
            "width"         : 80,
            "max_line_len"  : 32000,
            "ie_proof"      : true,
            "beautify"      : false,
            "bracketize"    : false,
            "semicolons"    : true,
            "comments"      : false
        }
    },

    "cache": true,
    "cache_async": true
}